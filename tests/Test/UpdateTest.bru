meta {
  name: Update Test
  type: http
  seq: 20
}

put {
  url: {{base_url}}/api/test/update-que-ans
  body: json
  auth: bearer
}

auth:bearer {
  token: {{jwt_token}}
}

body:json {
  {
    "test_id": {{test_id}},
    "questions_json": "{\"questions\": [\"Q1\", \"Q2\"]}",
    "answer_json": "{\"answers\": [\"A1\", \"A2\"]}"
  }
}

vars:pre-request {
  test_id: 1
}
